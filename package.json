{
  "name": "website2",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "prebuild": "node scripts/generate-build-info.mjs && node scripts/export-github-repos.mjs && node scripts/fetch-goodreads.mjs",
    "build": "nuxt build",
    "github:export": "node scripts/export-github-repos.mjs",
    "goodreads:fetch": "node scripts/fetch-goodreads.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,mjs,ts,vue,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,ts,vue,json,md}\"",
    "blog:import": "node scripts/blog/import.mjs",
    "blog:process": "DEBUG=true node scripts/processMarkdown.mjs",
    "blog:check-links": "CHECK_LINKS=true DEBUG=true node scripts/processMarkdown.mjs",
    "blog:fix-links": "CHECK_LINKS=true AUTO_FIX_LINKS=true DEBUG=true node scripts/processMarkdown.mjs",
    "blog": "npm run blog:import && npm run blog:process",
    "blog:preview": "DRY_RUN=true node scripts/blog/import.mjs",
    "blog:watch": "bash scripts/blog/watch.sh",
    "blog:publish": "bash scripts/blog/publish.sh",
    "publish": "bash scripts/blog/publish.sh",
    "blog:revert": "rm -rf content/processed/* && cp -r content/processed.backup/* content/processed/",
    "create-prediction": "node scripts/blog/create-prediction.mjs",
    "predict": "node scripts/predict-pro.mjs",
    "kalshi:test": "node scripts/kalshi-test.mjs",
    "kalshi:templates": "node scripts/generate-commentary-templates.mjs",
    "kalshi:calibration": "node scripts/calibration-analysis.mjs",
    "webmention": "node scripts/send-webmentions.mjs",
    "webmention:dry": "node scripts/send-webmentions.mjs --dry",
    "webmention:all": "node scripts/send-webmentions.mjs --all",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node .output/server/index.mjs",
    "clean": "rm -rf .nuxt dist .output content/processed/*"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.10.0",
    "@rehype-pretty/transformers": "^0.13.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/d3": "^7.4.3",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/node": "^24.0.10",
    "@types/sanitize-html": "^2.13.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "consola": "^3.4.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "gray-matter": "^4.0.3",
    "nuxt": "^4.0.3",
    "prettier": "^3.6.2",
    "rehype-pretty-code": "^0.14.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "shiki": "^3.7.0",
    "typescript": "^5.x.x",
    "unified": "^11.0.5",
    "vitest": "^4.0.15",
    "vue-eslint-parser": "^10.2.0",
    "zod": "^4.0.17"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.0.0",
    "@supabase/supabase-js": "^2.87.1",
    "@types/rss": "^0.0.32",
    "@vueuse/core": "^14.1.0",
    "animejs": "^4.2.2",
    "chalk": "^5.3.0",
    "chroma-js": "^3.2.0",
    "d3": "^7.9.0",
    "d3-scale-chromatic": "^3.1.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.0.1",
    "github-slugger": "^2.0.0",
    "glob": "^11.0.3",
    "h3": "^1.13.0",
    "kalshi-typescript": "^2.1.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.0",
    "ora": "^9.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rss": "^1.2.2",
    "sanitize-html": "^2.17.0",
    "striptags": "^3.2.0",
    "unist-util-visit": "^5.0.0",
    "vue": "^3.5.13"
  },
  "license": "UNLICENSED",
  "resolutions": {
    "@vercel/nft": "^0.27.4"
  }
}
